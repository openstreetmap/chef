<IfModule mod_info.c>

<Location /server-info>
    SetHandler server-info
<% node.ipaddresses.sort.each do |address| -%>
    Require ip <%= address %>
<% end -%>
    Require ip 127.0.1.1
    Require ip 127.0.0.1
    Require ip ::1
<% @hosts.each do |host| -%>
    Require ip <%= host %>
<% end -%>
</Location>

</IfModule>
