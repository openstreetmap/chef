# DO NOT EDIT - This file is being maintained by Chef

/var/log/apache2/*.log {
  daily
  size 1G
  missingok
  rotate 14
  compress
  delaycompress
  compresscmd /usr/bin/zstd
  compressext .zst
  compressoptions -T0 -11 --quiet --long --rsyncable
  uncompresscmd /usr/bin/unzstd
  notifempty
  create 640 root adm
  sharedscripts
  postrotate
    /bin/systemctl reload apache2
  endscript
}
