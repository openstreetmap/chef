/var/log/cinc/client.log {
  rotate 12
  weekly
  compress
  compresscmd /usr/bin/zstd
  compressext .zst
  compressoptions -T0 -11 --quiet --long
  uncompresscmd /usr/bin/unzstd
  postrotate
	systemctl try-restart cinc-client.service
  endscript
}
