[osm-pds]
aws_access_key_id = <%= @aws_credentials["pds_access_key_id"] %>
aws_secret_access_key = <%= @aws_credentials["pds_secret_access_key"] %>

[osm-pds-upload]
role_arn=arn:aws:iam::630658470130:role/osm-pds-upload-role
source_profile=osm-pds

[osm-osmdbt-state]
aws_access_key_id = <%= @aws_credentials["osmdbt_access_key_id"] %>
aws_secret_access_key = <%= @aws_credentials["osmdbt_secret_access_key"] %>

[osm-osmdbt-state-upload]
role_arn=arn:aws:iam::173189593406:role/osm-osmdbt-state-upload-role
source_profile=osm-osmdbt-state
